  "categories": [
    {
      "id": "People",
      "pattern": "\\b(worker|person|people|human|inspector|operator|foreman)\\b"
    },
    {
      "id": "Equipment & Vehicles",
      "pattern": "\\\\b(crane|excavator|bulldozer|loader|forklift|truck|dump truck|mixer|roller|grader|drone|uav|robot|car|dozer|tower crane|wheel loader)\\\\b"
    },
    {
      "id": "PPE & Safety",
      "pattern": "\\\\b(hard hat|reflective vest|vest|glove|goggle|mask|respirator|harness|traffic cone|barricade|barrier|sign|blue hardhat|hardhat|helmet|hi vis vest|high visibility vest|red hardhat|safety vest|white hardhat|yellow hardhat)\\\\b"
    },
    {
      "id": "Materials",
      "pattern": "\\\\b(concrete|cement|asphalt|steel|reinforcing bar|brick|wood|timber|glass|gypsum|drywall|insulation|soil|gravel|sand|fastener|reinforcing mesh|reinforcingmesh)\\\\b"
    },
    {
      "id": "Structural Components",
      "pattern": "\\\\b(beam|column|slab|girder|truss|joist|foundation|pile|pier|wall|deck|door|member|plate|ramp|roof)\\\\b"
    },
    {
      "id": "MEP Components",
      "pattern": "\\\\b(pipe|duct|hvac|valve|pump|cable|wire|panel|conduit|sprinkler|outlet|switch|meter|fan|fire suppression terminal|firesuppressionterminal|flow instrument|flowmeter|heatexchanger|humidifier|interceptor|light fixture|lightfixture|pipe fitting|pipe segment|pipefitting|pipesegment|protective device|protectivedevice|sanitary terminal|sanitaryterminal|sensor|space heater|spaceheater|switching device|switchingdevice|tank|unitary equipment|unitaryequipment|waste terminal|wasteterminal)\\\\b"
    },
    {
      "id": "Site & Temporary Works",
      "pattern": "\\b(scaffold|scaffolding|formwork|shoring|fence|road|lane|curb|site|stockpile|container|pallet)\\b"
    },
    {
      "id": "Defects & Damage",
      "pattern": "\\b(crack|spall|corrosion|rust|delamination|pothole|defect|damage|leak)\\b"
    }
  ],
  "label_rules": {
    "min_arc_px": 46,
    "min_arc_px_root": 70,
    "min_arc_px_zoom": 22
  },
  "exclusions": {
    "min_label_len": 2,
    "remove_terms": [
      "background",
