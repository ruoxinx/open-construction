      "other",
      "others",
      "tbd",
      "thing",
      "things",
      "todo",
      "unknown",
      "unlabeled",
      "unlabelled",
      "void"
    ],
    "remove_patterns": [
      "^\\s*$",
      "^\\d+$",
      "^\\W+$",
      "^(?:bg|bkg|background)$",
      "^(?:other|unknown|misc|miscellaneous)$",
      "^no\\s*(?:label|labels)?$",
      "^(?:n\\/?a|na|none|null)$",
      "^unlabeled$",
      "^unlabelled$"
    ]
  },
