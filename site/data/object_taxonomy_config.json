{
  "taxonomy": [
    {
      "id": "People",
      "children": [
        {
          "id": "Construction Personnel",
          "pattern": "\\b(worker|person|people|human|laborer|foreman|operator)\\b"
        },
        {
          "id": "Inspection & Safety Staff",
          "pattern": "\\b(inspector|safety)\\b"
        }
      ]
    },
    {
      "id": "PPE & Safety Equipment",
      "children": [
        {
          "id": "Head Protection",
          "pattern": "\\b(hard\\s*hat|hardhat|helmet)\\b"
        },
        {
          "id": "Eye & Face Protection",
          "pattern": "\\b(goggle|face\\s*shield|face\\s*protection)\\b"
        },
        {
          "id": "Hand Protection",
          "pattern": "\\b(glove)\\b"
        },
        {
          "id": "Foot Protection",
          "pattern": "\\b(boot|safety\\s*boot)\\b"
        },
        {
          "id": "Fall Protection",
          "pattern": "\\b(harness|lanyard|lifeline)\\b"
        },
        {
          "id": "High-Visibility Apparel",
          "pattern": "\\b(hi\\s*vis|high\\s*visibility|reflective\\s*vest|safety\\s*vest|vest)\\b"
        },
        {
          "id": "Traffic Control & Barricades",
          "pattern": "\\b(traffic\\s*cone|cone|barricade|barrier)\\b"
        },
        {
          "id": "Safety Signage",
          "pattern": "\\b(safety\\s*sign|warning\\s*sign|fire\\s*exit\\s*sign|fire\\s*door\\s*sign|road\\s*sign)\\b"
        }
      ]
    },
    {
      "id": "Equipment & Vehicles",
      "children": [
        {
          "id": "Earthmoving Equipment",
          "pattern": "\\b(excavator|bulldozer|dozer|loader|skid\\s*steer|grader|backhoe)\\b"
        },
        {
          "id": "Lifting & Hoisting Equipment",
          "pattern": "\\b(crane|tower\\s*crane|truck\\s*crane|mobile\\s*crane|hook|outtrigger)\\b"
        },
        {
          "id": "Concrete Equipment",
          "pattern": "\\b(concrete\\s*mixer|mixer\\s*truck|pump\\s*truck)\\b"
        },
        {
          "id": "Compaction & Paving",
          "pattern": "\\b(roller|compactor)\\b"
        },
        {
          "id": "Transport Vehicles",
          "pattern": "\\b(dump\\s*truck|truck|car|bus)\\b"
        },
        {
          "id": "Attachments & Components",
          "pattern": "\\b(bucket|boom\\s*arm|arm\\s*bucket)\\b"
        }
      ]
    },
    {
      "id": "Tools",
      "children": [
        {
          "id": "Hand Tools",
          "pattern": "\\b(hammer|wrench|spanner|shovel|trowel|saw|knife)\\b"
        },
        {
          "id": "Power Tools",
          "pattern": "\\b(drill|grinder|cutter)\\b"
        },
        {
          "id": "Measuring & Layout Tools",
          "pattern": "\\b(level|measuring|survey)\\b"
        }
      ]
    },
    {
      "id": "Temporary Works & Site Logistics",
      "children": [
        {
          "id": "Scaffolding & Access",
          "pattern": "\\b(scaffold|scaffolding|ladder)\\b"
        },
        {
          "id": "Formwork & Shoring",
          "pattern": "\\b(formwork|shoring)\\b"
        },
        {
          "id": "Fencing & Barriers",
          "pattern": "\\b(fence|fencing)\\b"
        },
        {
          "id": "Storage & Waste",
          "pattern": "\\b(container|skip\\s*bin|waste\\s*container|rubbish\\s*bin|pallet)\\b"
        },
        {
          "id": "Roadway & Site Elements",
          "pattern": "\\b(road|lane|curb|zebra\\s*crossing|bus\\s*stop|side\\s*path)\\b"
        }
      ]
    },
    {
      "id": "Structural Elements",
      "children": [
        {
          "id": "Foundations",
          "pattern": "\\b(foundation|footing|pile|pier)\\b"
        },
        {
          "id": "Columns & Vertical Members",
          "pattern": "\\b(column|pillar)\\b"
        },
        {
          "id": "Beams & Girders",
          "pattern": "\\b(beam|girder|i\\s*beam)\\b"
        },
        {
          "id": "Slabs & Decks",
          "pattern": "\\b(slab|deck)\\b"
        },
        {
          "id": "Connections & Reinforcement",
          "pattern": "\\b(rebar|reinforcing\\s*bar|reinforcement\\s*bar|reinforcing\\s*mesh|fastener|plate|brace|bracing)\\b"
        }
      ]
    },
    {
      "id": "Architectural & Envelope Elements",
      "children": [
        {
          "id": "Walls & Partitions",
          "pattern": "\\b(wall|cmu\\s*wall|partition)\\b"
        },
        {
          "id": "Doors & Windows",
          "pattern": "\\b(door|window|curtain\\s*wall|curtainwall)\\b"
        },
        {
          "id": "Roofs",
          "pattern": "\\b(roof)\\b"
        },
        {
          "id": "Interior Finishes",
          "pattern": "\\b(tile|carpet|wallpaper|plaster|gypsum|drywall|ceiling)\\b"
        },
        {
          "id": "Openings",
          "pattern": "\\b(opening|opening\\s*hole|opening\\s*corner)\\b"
        }
      ]
    },
    {
      "id": "MEP Systems",
      "children": [
        {
          "id": "Electrical",
          "pattern": "\\b(electric|electrical|lighting|light|luminaire|panel|distribution board|switch|outlet|receptacle|breaker|conduit|cable|wire|tray|junction box|meter|transformer|generator|ups|switchgear|motor)\\b"
        },
        {
          "id": "HVAC",
          "pattern": "\\b(hvac|air\\s*terminal|air\\s*terminal\\s*box|duct|diffuser|grille|damper|ahu|chiller|boiler|fan\\b|vav\\b|fcu\\b|heat\\s*exchanger|humidifier|air\\s*conditioning|heat\\s*recovery)\\b"
        },
        {
          "id": "Plumbing",
          "pattern": "\\b(plumbing|pipe|pipe\\s*fitting|pipe\\s*segment|valve|pump\\b|tank\\b|sanitary|waste\\s*terminal|hydrant\\b|fixture|toilet|sink|faucet|water\\s*heater|interceptor)\\b"
        },
        {
          "id": "Fire Protection",
          "pattern": "\\b(fire\\s*alarm|alarm\\b|sprinkler|standpipe|hose\\s*reel|extinguisher|fire\\s*hydrant|suppression|call\\s*point|smoke\\s*detector|detector)\\b"
        }
      ]
    },
    {
      "id": "Materials & Products",
      "children": [
        {
          "id": "Concrete & Cementitious",
          "pattern": "\\b(concrete|cement|asphalt|mortar)\\b"
        },
        {
          "id": "Steel & Metals",
          "pattern": "\\b(steel|metal)\\b"
        },
        {
          "id": "Wood & Timber",
          "pattern": "\\b(wood|timber)\\b"
        },
        {
          "id": "Masonry",
          "pattern": "\\b(brick|masonry|stone)\\b"
        },
        {
          "id": "Insulation & Membranes",
          "pattern": "\\b(insulation|membrane|pe\\s*membrane|foaming\\s*insulation)\\b"
        },
        {
          "id": "Glass & Plastics",
          "pattern": "\\b(glass|plastic)\\b"
        },
        {
          "id": "Surface Treatments",
          "pattern": "\\b(exposed\\s*aggregate|broomiong|paint|painting|coating)\\b"
        }
      ]
    },
    {
      "id": "Defects & Damage",
      "children": [
        {
          "id": "Cracking",
          "pattern": "\\b(crack|cracking)\\b"
        },
        {
          "id": "Spalling & Delamination",
          "pattern": "\\b(spall|spalling|delamination)\\b"
        },
        {
          "id": "Corrosion",
          "pattern": "\\b(corrosion|rust)\\b"
        },
        {
          "id": "Potholes & Raveling",
          "pattern": "\\b(pothole|ravelling|rutting)\\b"
        }
      ]
    },
    {
      "id": "Work Activities & Postures",
      "children": [
        {
          "id": "Equipment Operations",
          "pattern": "\\b(excavators\\s*(dump|idle|move|load|swing)|dump\\s*trucks\\s*(fill|move))\\b"
        },
        {
          "id": "Manual Tasks",
          "pattern": "\\b(picking|placing|plastering|tiling|masonry\\s*work|handcart\\s*work)\\b"
        },
        {
          "id": "Access & Climbing",
          "pattern": "\\b(climb|climbing|stairs|kneel|crouch|leaning|handsup)\\b"
        },
        {
          "id": "Unsafe Events",
          "pattern": "\\b(fall\\s*detected|fall\\s*down|unsafe\\s*activity|no\\s*hardhat|not\\s*wearing\\s*safety)\\b"
        }
      ]
    },
    {
      "id": "Signage & Information",
      "children": [
        {
          "id": "Regulatory & Warning Signage",
          "pattern": "\\b(sign|billboard|advertising\\s*board|fire\\s*exit\\s*sign|traffic\\s*signal)\\b"
        }
      ]
    },
    {
      "id": "Environment & Conditions",
      "children": [
        {
          "id": "Airborne Particulates",
          "pattern": "\\b(dust|smoke|particle\\s*monitor)\\b"
        },
        {
          "id": "Environment Context",
          "pattern": "\\b(sky|tree|foliage|water)\\b"
        }
      ]
    },
    {
      "id": "Non-Visual / Metadata",
      "children": [
        {
          "id": "Color & Appearance",
          "pattern": "\\b(color|painted)\\b"
        },
        {
          "id": "Quantity & Status",
          "pattern": "\\b(quantity|status|position|movement|energy|education|health|food)\\b"
        }
      ]
    }
  ],
